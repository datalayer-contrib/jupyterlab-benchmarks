{
  "name": "@jupyterlab/benchmarks",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:jlab": "cd .. && jlpm run build",
    "build:benchmark": "tsc",
    "kill:jlab": "fkill -s :9999",
    "start:jlab": "cd .. && jupyter lab --dev --no-browser --port 9999 --LabApp.password= --LabApp.token=",
    "start:benchmark": "wait-on http-get://localhost:9999/lab && node --experimental-modules lib/index.js",
    "start:all": "npm-run-all kill:jlab --parallel start:jlab start:benchmark --race",
    "start:analysis": "cd .. && jupyter lab --dev benchmarks/analysis.vl.json",
    "all": "npm-run-all build:* start:all start:analysis"
  },
  "dependencies": {
    "playwright": "0.11.1"
  },
  "devDependencies": {
    "fkill-cli": "6.0.1",
    "npm-run-all": "4.1.5",
    "typescript": "~3.7.3",
    "wait-on": "4.0.1"
  }
}
